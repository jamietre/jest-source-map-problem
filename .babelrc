{
  "presets": ["es2015", "es2016", "es2017", "react", "stage-2"],
  "plugins": [
    "transform-decorators-legacy",
    "transform-async-functions",
    [
      "transform-builtin-extend",
      {
        "globals": [ "Error" ],
        "approximate": true
      }
    ]
  ],
  "sourceMaps": "inline"
}
